DECLARE
L employees.LAST_NAME%TYPE;
S employees.SALARY%TYPE;
I employees.employee_ID%TYPE:=:ID;
BEGIN 
SELECT LAST_NAME, SALARY INTO L,S  
FROM employees
WHERE employee_ID = I ;
DBMS_OUTPUT.PUT_LINE('LASTNAME: '||L|' '|'SALARY = '||S);


EXCEPTION 
WHEN NO_DATA_FOUND THEN 
DBMS_OUTPUT.PUT_LINE('NO DONT IS HER>>EROR!...<< PLESE AGIEN <!>');


END;


DECLARE
N EMPLOYEES.LAST_NAME%TYPE;
S EMPLOYEES.SALARY%TYPE;
I EMPLOYEES.EMPLOYEE_ID%TYPE:=:EMPLOYEE_ID;
BEGIN
SELECT LAST_NAME, SALARY INTO N,S 
FROM EMPLOYEES
WHERE EMPLOYEE_ID = I;
DBMS_OUTPUT.PUT_LINE('NAME: '||N);
DBMS_OUTPUT.PUT_LINE('SALARY: '||S);
EXCEPTION
WHEN NO_DATA_FOUND THEN 
DBMS_OUTPUT.PUT_LINE(' EMPLOYEE ID IS NOT CORRECT');
END;


CREATE OR REPLACE PROCEDURE P1 
IS 
BEGIN 
DBMS_OUTPUT.PUT_LINE('HELLO <3');
END;
-- استعداء الاجراء السابق 
BEGIN
P1;
END;



DECLARE 
I EMPLOYEES.EMPLOYEE_ID%TYPE:=:ENTER;
L EMPLOYEES.LAST_NAME%TYPE;
S EMPLOYEES.SALARY%TYPE;
BEGIN
SELECT LAST_NAME, SALARY INTO L,S FROM EMPLOYEES 
WHERE EMPLOYEE_ID = I ;
DBMS_OUTPUT.PUT_LINE('LASTNAME'||L);
DBMS_OUTPUT.PUT_LINE('SALARY = '||S);
EXCEPTION 
WHEN NO_DATA_FOUND THEN
DBMS_OUTPUT.PUT_LINE('EROER...!');



END; 





CREATE OR REPLACE PROCEDURE MSG
IS 
BEGIN 
DBMS_OUTPUT.PUT_LINE('HELLO');
END; 


BEGIN
MSG;
END;



CREATE OR REPLACE PROCEDURE SS(X IN NUMBER, Y IN NUMBER,rslt OUT NUMBER )
IS 
BEGIN
rslt = X+Y;
END;
END;


DECLARE
A NUMBER:=:A;
B NUMBER:=:B;
C NUMBER;
BEGIN
SS(A,B,C);
DBMS_OUTPUT.PUT_LINE('المجوع = ' ||C);
END;


